#!/usr/bin/gnuplot
reset
set terminal png

set xdata
set xlabel "connections"

set ylabel "reqs/second"

set title "Performance vs concurrenct connections" 
set key reverse Left outside
set grid

set style data lines

plot 'results.data' using 1:2 title "Async", \
"" using 1:3 title "Sync", \
"" using 1:4 title "Async_Sync"
